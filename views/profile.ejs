<h2>Profile Page</h2>

<h3>Welcome to your PROFILE  </h3>
<%= currentUser.name %>
<ul>
    <% lunch.forEach(lunch => { %>     
    <li><a href="/profile/<%= lunch.id %>"><%= lunch.name%></a></li>
    <form method="POST" action="/profile/<%= lunch.id %>?_method=DELETE">
        <button type="submit"  value="DELETE">Delete Lunch</button>
      </form>
     
    <% }); %>
</ul>
